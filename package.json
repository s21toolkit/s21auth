{
  "name": "@s21toolkit/auth",
  "description": "Auth library for edu.21-school.ru",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm lint && pnpm build:tsup",
    "lint:tsc": "tsc",
    "lint": "pnpm lint:biome && pnpm lint:tsc",
    "prepublishOnly": "pnpm build",
    "release": "npm version -m 'chore(release): %s' --",
    "postversion": "git push && git push --tags",
    "build:tsup": "tsup",
    "lint:biome": "biome check .",
    "fix:biome": "biome check --apply .",
    "fix": "pnpm fix:biome"
  },
  "files": [
    "build/*",
    "README.md"
  ],
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.1",
    "@s21toolkit/shared": "^1.4.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/uuid": "^9.0.8",
    "tsup": "^8.1.0",
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s21toolkit/s21client-ts.git"
  },
  "private": false,
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts"
}
